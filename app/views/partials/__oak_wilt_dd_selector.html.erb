<%= form_tag({action: :get_oak_wilt_dd}, {class: "form__oak_wilt"}) do %>
  <div class="form-row">
    <label for="latitude">Latitude</label>
    <%= select_tag :latitude, options_for_select(latitudes) %>
  </div>
  <div class="form-row">
    <label for="longitude">West longitude</label>
    <%= select_tag :longitude, options_for_select(longitudes) %>
    <a href="https://www.lifewire.com/latitude-longitude-coordinates-google-maps-1683398" target="_blank" class="helper-link">How to find coordinates from Google Maps</a>
  </div>
  <div class="form-row">
  <label for="end_date">End Date</label>
  <div>
    <%= date_select :grid_date, :end_date, start_year: 2000, default: 1.day.ago, end_year: Time.now.year %>
  </div>
  </div>
  <input type="hidden" name="model_type" value="oak_wilt"/>
  <input type="hidden" name="method" value="Sine"/>
  <input type="hidden" name="base_temp" value="41" />
  <input type="hidden" name="upper_temp" value="None" />
  <%= submit_tag("Predict Oak Wilt Vector Emergence Status", class: "button")%>
<% end %>
